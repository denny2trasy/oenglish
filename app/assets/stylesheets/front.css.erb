@charset "utf-8";
/* CSS Document */
* { padding:0; margin:0; }
img { border:0; }
ul, ol { list-style:none }
a { color:#333333; text-decoration:none; }
a:hover, .box_picture a:hover, .lesson a:hover, .liveclass a:hover { color:#f73337; }
body { font-family:"微软雅黑"; font-size:12px; font-weight:normal; background:#f9f9f9; }
h1, h2, h3, h4, h5, h6, .curriculum, .head ul a, .activation, input, select { font-family:"微软雅黑"; font-weight:normal; }
/**/
.head { background:url( <%= asset_url("front/picture02_02.gif") %>); height:115px; padding:3px 0 0 0; }
.head div { width:1005px; margin:0 auto; }
.head h2 { float:left; padding:0 0 0 5px; }
.head i { float:right; height:52px; }
.head ul { border-left:1px solid #e8e8ea; float:left; width:1004px; }
.head ul li { float:left; }
.head ul a { float:left; width:106px; text-align:center; border-right:1px solid #e8e8ea; font-size:17px; line-height:59px; }
/**/
.center { width:1005px; margin:0 auto; color:#333333; }
.banner { height:177px; float:left; width:100%; }
.banner i { position:absolute; margin:-3px 0 0 0; }
.box_left { margin:10px 0 0 0; width:680px; float:left; }
.courses { border:1px solid #e0e0e0; background:#fff; border-bottom:1px solid #c8c8c8; height:285px; float:left; width:678px; padding-bottom:15px; }
.courses h2 { font-size:20px; background:url(<%= asset_url('front/picture03_06.gif' )%>); height:43px; padding:20px 0 0 32px; }
.box_picture { width:618px; float:left; margin:14px 0 0 33px; font-size:14px; line-height:20px; color:#828282; display:inline; }
.box_picture i { float:left; margin:0 17px 0 0; }
.box_picture a { font-size:16px; color:#0b2272; }
.box_picture p { height:127px; border-top:1px solid #CCCCCC; margin-top:10px; padding-top:10px; }
.box_picture span { width:326px; float:left; }
.box_picture span img { float:right }
/**/
.group { margin:13px 0 0 0; height:auto; }
.group h2 { border-bottom:1px solid #e8e8e8; background:none; }
.lesson { width:618px; float:left; margin:14px 0 0 33px; display:inline; border-bottom:1px solid #e8e8e8; padding:15px 0 29px 0; font-size:16px; line-height:20px; }
.lesson a { color:#828282 }
/**/
.status { margin:13px 0 0 0; height:auto; }
.curriculum { border-bottom:1px solid #f3f3f3; font-size:14px; width:100%; float:left; color:#828282; line-height:39px; }
.pack { padding:0 0 0 56px; width:202px; float:left; }
.valid { width:136px; text-align:center; float:left; }
.lessons { width:136px; text-align:center; float:left; }
.operate { width:148px; text-align:center; float:left; }
.oenglish { padding:0 0 0 44px; width:214px; float:left; background:url( <%= asset_url("front/picture05_03.gif") %> ) no-repeat 16px 12px; }
#display { color:#41bbe0; padding-bottom:12px; }
.classification { width:604px; float:left; margin:0 0 0 44px; border:1px solid #e3e3e3; border-radius:5px; color:#828282; display:inline; }
.classification td { border-top:1px solid #e3e3e3; }
.classification .bj2014 td { background:#f8f8f8; border-top:none; }
/**/
.box_right { width:314px; float:left; margin:10px 0 0 11px; color:#828282; }
.course { float:left; }
.activation { background:#fff; width:313px; height:154px; border-bottom:1px solid #c8c8c8; border-right:1px solid #e0e0e0; float:left; font-size:14px; line-height:22px; }
.activation span { background:url(<%= asset_url("front/picture07_03.gif") %>) no-repeat 21px 19px; display:block; padding:15px 0 0 56px }
.activation .phone { background:url(<%= asset_url("front/picture07_14.gif") %>) no-repeat 21px 19px }
.activation i { display:block; border-bottom:1px solid #f0f0f0; border-top:1px solid #f0f0f0; padding:9px 0; margin:15px 0 0 0; }
.activation input { float:left; width:170px; background:url(<%= asset_url("front/picture07_10.gif") %>); border:1px solid #bbbbbb; height:27px; line-height:27px; color:#333333; font-size:12px; padding:0 0 0 4px; margin:1px 10px 0 20px; }
.liveclass { widows:314px; float:left; margin:13px 0 0 0; }
.liveclass i { height:90px; display:block; }
.liveclass ul { background:#fff; border:1px solid #e0e0e0; border-top:none; padding:10px 16px 16px 16px; width:280px; float:left; }
.liveclass span { font-size:18px; color:#f73337; }
.liveclass li { font-size:14px; line-height:30px; }
.liveclass a { background:url(<%= asset_url("front/picture08_07.gif") %>) no-repeat right; padding-right:10px; color:#828282; }
/**/
.footer { background:url(<%= asset_url("front/picture10_03.gif") %>); height:194px; width:100%; float:left; margin:13px 0 0 0; }
.footer div { width:1005px; margin:0 auto; text-align:center; }
.footer i { display:block; border-bottom:1px solid #fff; padding:20px 0; margin:0 0 15px 0; }
/**/
.onlinecourses { margin:10px 0 0 0; border:1px solid #e0e0e0; background:#fff; border-bottom:1px solid #c8c8c8; float:left; width:1003px; }
.onlinecourses h2 { font-size:20px; background:url(<%= asset_url("front/picture03_06.gif") %>); height:43px; padding:20px 0 0 32px; }
.onlinecourses div { width:939px; float:left; margin:0 32px 0 32px; display:inline; overflow:hidden; }
.onlinecourses ul { width:1100px; line-height:20px; color:#828282; margin:-1px 0 0 0; }
.onlinecourses li { float:left; width:170px; font-size:16px; border-top:1px solid #dcdcdc; padding:17px 0; }
.onlinecourses a { color:#828282; }
.onlinecourses a:hover, .onlinecourses .tips a { color:#0b2272 }
.f2014 { line-height:39px; font-size:14px; width:1003px; margin:13px 0 0 0; }
.f2014 td { padding:0 30px; background:#fff; }
.f2014 .bj_2014 td { background:#f8f8f8; }
/**/
.box_top{ background:url(<%= asset_url("front/picture15_02.gif") %>); height:52px;}
.box_top div { width:1005px; margin:0 auto; }
.box_top i img{ float:right;}
.ielts{ color:#333333; width:220px; height:582px; float:left; background:url(<%= asset_url("front/picture16_03.gif") %>); border-left:1px solid #d9dce2; border-right:1px solid #d9dce2; border-bottom:1px solid #d9dce2; box-shadow: #d9dce2 0px 4px 6px;}
.ielts h2{ height:140px; float:left; width:100%;}
.ielts i{ position:absolute; top:10px; margin:0 0 0 15px;}
.ielts div{ height:360px; float:left;}
.ielts ul { margin:0 0 0 15px; display:inline; width:190px; float:left; border-bottom:1px solid #e2e2e2; padding:0 0 20px 0;}
.ielts li{ width:100%; float:left; margin:15px 0 0 0;}
.ielts li span{ float:right}
.ielts li span a{ color:#0b2272;}
.ielts .login{ 
	color:#fff; 
/*	background:url(<%= asset_url("/front/picture13_03.gif") %>); */
	width:190px; 
	line-height:36px; 
	font-size:14px; 
	float:left; 
	text-align:center; 
	font-weight:bold;
	background-color: #df002f;
}
.login:hover{ color:#fff}
#flash_notice {
	height: 20px;
}
.input{ width:184px; height:34px; line-height:24px; border:1px solid #adb6c9; font-size:14px; color:#333333; padding:0 0 0 4px;}
.ip2014{ float:left; margin:2px 10px 0 0; *margin:-2px 6px 0 -3px;}
.ielts em{ float:left; margin:0 0 0 4px;}
.registration{ height:582px; float:left; width:780px; background:url(<%= asset_url("front/picture13_03.jpg") %>) no-repeat bottom right #fff; border-right:1px solid #d9dce2; border-bottom:1px solid #d9dce2; box-shadow: #d9dce2 0px 4px 10px;}
.tmbj{ color:#333333; background:url(<%= asset_url("front/bj.png") %>); width:324px; float:left; display:inline; margin:23px 0 0 33px; font-size:14px;}
.tmbj ul li{ width:100%; float:left; margin:14px 0 0 0; _margin:12px 0 0 0;}
.tmbj b{ font-size:15px}
.tmbj ul select{ font-size:14px; color:#333333; border:1px solid #adb6c9; padding:2px;}
.username{ width:82px; text-align:right; float:left; line-height:27px;}
.inputbox{ width:226px; float:left;}
.inputbox p{ font-size:12px; color:#999999; margin:5px 0 0 0;}
.inputbox p a{ color:#0b2272;}
.input_box{ width:216px; height:27px; border:1px solid #adb6c9; font-size:14px; line-height:27px; color:#333333; padding:0 0 0 4px;}
.signupnow{ background:url(<%= asset_url("front/picture18_03.gif") %>);  color:#fff; width:175px; line-height:36px; font-size:14px; float:left; text-align:center; font-weight:bold;}
.signupnow:hover{ color:#fff}



