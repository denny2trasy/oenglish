<h2><%=t(:sign_up)%></h2>

<%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
  <%= devise_error_messages! %>

  <div><%= f.label t(:email) %><br />
  <%= f.email_field :email, autofocus: true %></div>

  <div><%= f.label t(:password) %><br />
    <%= f.password_field :password, autocomplete: "off" %></div>

  <div><%= f.label t(:password_confirmation) %><br />
    <%= f.password_field :password_confirmation, autocomplete: "off" %></div>

  <div><%= f.label t(:name) %><br />
    <%= f.text_field :name %></div>

  <div><%= f.label t(:login) %><br />
    <%= f.text_field :login %></div>

  <div><%= f.label t(:qq) %><br />
    <%= f.text_field :qq %></div>

  <div><%= f.label t(:weixin) %><br />
    <%= f.text_field :weixin %></div>

  <div><%= f.label t(:mobile) %><br />
    <%= f.text_field :mobile %></div>

  <div><%= f.label t(:agent) %><br />
    <%= f.select :agent_id, agents_list %></div>

	<%= f.hidden_field :u_type, :value => "student"%>


  <div><%= f.submit t(:sign_up) %></div>
<% end %>

<%#= render "devise/shared/links" %>
